    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@tsconfig/svelte": "^5.0.6",
    "@types/node": "^22.10.5",
    "@types/papaparse": "^5.5.2",
    "dotenv": "^17.2.3",
    "supabase": "^2.72.8",
    "svelte": "^5.47.1",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vite": "^6.4.1"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.0.0",
    "@supabase/supabase-js": "^2.78.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@tcgdex/sdk": "^2.7.1",
    "flexsearch": "^0.8.212",
    "glob": "^10.5.0",
    "idb": "^8.0.3",
    "lucide-svelte": "^0.562.0",
    "p-limit": "^6.2.0",
    "papaparse": "^5.5.3",
    "svelte-sonner": "^1.0.7",
    "tailwindcss": "^4.1.18"
  }
